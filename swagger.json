  "components": {
    "schemas": {
      "BlueprintChapterDto": {
        "type": "object",
        "properties": {
          "chapter": {
            "type": "integer",
            "format": "int32"
          },
          "easyCount": {
            "type": "integer",
            "format": "int32"
          },
          "mediumCount": {
            "type": "integer",
            "format": "int32"
          },
          "hardCount": {
            "type": "integer",
            "format": "int32"
          },
          "veryHardCount": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ChangePasswordDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "format": "email",
            "nullable": true
          },
          "newPassword": {
            "type": "string",
            "nullable": true
          },
          "oldPassword": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CheckOtpDto": {
        "required": [
          "email",
          "otp"
        ],
        "type": "object",
        "properties": {
          "otp": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "format": "email",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateClassDto": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "teacherId": {
            "type": "integer",
            "format": "int32"
          },
          "subjectId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "CreateExamBlueprintDto": {
        "required": [
          "chapters"
        ],
        "type": "object",
        "properties": {
          "subjectId": {
            "type": "integer",
            "format": "int32"
          },
          "chapters": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/BlueprintChapterDto"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateExamForStudentDto": {
        "type": "object",
        "properties": {
          "examId": {
            "type": "integer",
            "format": "int32"
          },
          "studentId": {
            "type": "integer",
            "format": "int32"
          },
          "durationMinutes": {
            "type": "integer",
            "format": "int32"
          },
          "startTime": {
            "type": "string",
            "format": "date-time"
          },
          "endTime": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "CreateExamForTeacherOrAdmin": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "classId": {
            "type": "integer",
            "format": "int32"
          },
          "blueprintId": {
            "type": "integer",
            "format": "int32"
          },
          "durationMinutes": {
            "type": "integer",
